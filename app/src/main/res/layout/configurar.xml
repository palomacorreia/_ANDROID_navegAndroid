<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_alignStart="@+id/textView"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignEnd="@+id/editText"
        android:layout_alignRight="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_marginTop="353dp"
        android:layout_marginEnd="326dp"
        android:src="@drawable/seta" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_alignStart="@+id/textView"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignEnd="@+id/editText"
        android:layout_alignRight="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_marginTop="186dp"
        android:layout_marginEnd="317dp"
        android:src="@drawable/seta" />

    <RadioGroup
        android:id="@+id/formato"
        android:layout_width="412dp"
        android:layout_height="109dp"
        android:layout_alignStart="@+id/decimal"
        android:layout_alignLeft="@+id/minutodecimal"
        android:layout_alignEnd="@+id/segundo"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-72dp"
        android:layout_marginLeft="-72dp"
        android:layout_marginTop="241dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="-42dp"
        android:weightSum="1"
        tools:ignore="NotSibling">

        <RadioButton
            android:id="@+id/decimal"
            android:layout_width="253dp"
            android:layout_height="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="0.13"
            android:checked="false"
            android:text="Grau Decimal"
            android:textSize="14sp" />

        <RadioButton
            android:id="@+id/minutodecimal"
            android:layout_width="253dp"
            android:layout_height="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="0.13"
            android:checked="false"
            android:text="Grau-Minuto Decimal"
            android:textSize="14sp" />

        <RadioButton
            android:id="@+id/segundo"
            android:layout_width="253dp"
            android:layout_height="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="0.13"
            android:checked="false"
            android:text="Grau-Minuto-Segundo Decimal"
            android:textSize="14sp" />

    </RadioGroup>

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/radioGroup"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="344dp"
        android:layout_marginEnd="153dp"
        android:layout_marginBottom="10dp"
        android:text="New Button" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="315dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="507dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Formato das Coordenadas:" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="334dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Unidade de apresentação:" />

    <RadioGroup
        android:id="@+id/apresentacao"
        android:layout_width="359dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/km"
        android:layout_alignLeft="@+id/mph"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-2dp"
        android:layout_marginLeft="-17dp"
        android:layout_marginTop="408dp"
        android:layout_marginEnd="-2dp"
        android:layout_marginRight="-42dp"
        android:layout_marginBottom="255dp"
        android:weightSum="1"
        tools:ignore="NotSibling">

        <RadioButton
            android:id="@+id/km"
            android:layout_width="253dp"
            android:layout_height="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="0.13"
            android:checked="false"
            android:text="KM/h"
            android:textSize="14sp" />

        <RadioButton
            android:id="@+id/mph"
            android:layout_width="253dp"
            android:layout_height="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="0.13"
            android:checked="false"
            android:text="Mph"
            android:textSize="14sp" />

    </RadioGroup>


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="23dp"
        android:layout_height="28dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="350dp"
        android:layout_marginBottom="467dp"
        app:srcCompat="@drawable/one" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="23dp"
        android:layout_height="26dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="351dp"
        android:layout_marginBottom="435dp"
        app:srcCompat="@drawable/two" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="23dp"
        android:layout_height="26dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="350dp"
        android:layout_marginBottom="404dp"
        app:srcCompat="@drawable/three" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="146dp"
        android:layout_height="104dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="291dp"
        android:layout_marginBottom="556dp"
        app:srcCompat="@drawable/configurar" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="275dp"
        android:layout_height="59dp"
        android:layout_above="@+id/editText"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="27dp"
        app:srcCompat="@drawable/config" />

</RelativeLayout>